		"@types/rimraf": "3.0.0",
		"babel-eslint": "10.0.3",
		"babel-loader": "8.1.0",
		"cpx": "1.5.0",
		"eslint": "6.8.0",
		"esm": "3.2.25",
		"execa": "4.0.3",
		"husky": "4.2.5",
		"jest": "26.1.0",
		"npm-run-all": "4.1.5",
		"rimraf": "2.6.3",
		"semantic-release": "17.1.1",
		"ts-jest": "26.2.0",
		"ts-loader": "6.2.1",
		"ts-node": "8.10.2",
		"typescript": "3.9.6"
	}
